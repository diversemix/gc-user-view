    "@testing-library/jest-dom": "^4.0.0",
    "@types/jest": "^24.0.15",
    "@types/jquery": "^3.3.31",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-loader": "8.0.6",
    "codeceptjs": "^2.3.5",
    "eslint": "^6.0.1",
